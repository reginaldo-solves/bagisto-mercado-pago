{
    "name": "reginaldo/mercado-pago",
    "type": "bagisto-package",
    "description": "MÃ©todo de pagamento Mercado Pago para Bagisto",
    "keywords": ["bagisto", "payment", "mercado-pago", "pix", "credit-card", "boleto", "brazil"],
    "license": "MIT",
    "authors": [
        {
            "name": "Reginaldo",
            "email": "reginaldo@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "mercadopago/dx-php": "^2.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\MercadoPago\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webkul\\MercadoPago\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webkul\\MercadoPago\\Providers\\MercadoPagoServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
